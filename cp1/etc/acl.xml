            </resource>   
                 <resource id="Cp_User::cron_menu" title="Cron">
                     <resource id="Cp_User::cron_list" title="CronList" translate="title" sortOrder="10" />
                 </resource>  

               
        </resource>
    </resources>

    </acl>
</config>
