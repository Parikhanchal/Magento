<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
    <resources>
        <resource id="Magento_Backend::admin">
            <resource id="Cp_User::user_details" title="UserDetails">
                        <resource id="Cp_User::user_grid" title="Grid" translate="title" sortOrder="10" />
                        <resource id="Cp_User::user_pinfo" title="Personal" translate="title" sortOrder="30" />
